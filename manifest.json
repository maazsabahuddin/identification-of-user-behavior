{
  "name": "Behaviour Track",
  "description": "Building Extension!",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "/images/get_started_techwards.png",
        "32": "/images/get_started_techwards.png",
        "48": "/images/get_started_techwards.png",
        "128": "/images/get_started_techwards.png"
    }
  },
  "icons": {
    "16": "/images/get_started_techwards.png",
    "32": "/images/get_started_techwards.png",
    "48": "/images/get_started_techwards.png",
    "128": "/images/get_started_techwards.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}